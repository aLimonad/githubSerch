@{
    ViewData["Title"] = "Home Page";
}

<div class="col col-mb-12 col-8 col-dt-9" id="abitureBlock">
    <div class="white-box col-margin-bottom padding-box">
        <div id="work_range">
            <form  data-ajax-url="/Home/GetProjects" data-ajax="true" data-ajax-mode="replace" data-ajax-update="#results" id="form0" method="get">    
                <label style="margin-right: 5px;">Название проекта: </label>
                <input name="projectName" id="projectName" class="form-control" style="margin-bottom: 10px;"/>
                <input type="submit" value="Поиск" class="btn btn-outline-dark" style="margin-bottom: 10px;" />
            </form>
            <div id="results">

            </div>
        </div>
    </div>
</div>